# Vertex AI Agent Builder Environment Configuration Example
# Copy this file to .env and fill in your specific values

# Project Configuration
export PROJECT_ID="your-project-id"
export PROJECT_NUMBER="your-project-number"
export REGION="us-central1"
export LOCATION="global"

# Storage Configuration
export BUCKET_BASE="gs://your-project-id-vertex-agents"

# API Keys
# Anthropic
export ANTHROPIC_API_KEY="your-anthropic-api-key"

# Deepseek
export DEEPSEEK_R1_API_KEY="your-deepseek-api-key"

# Google Cloud
export GEMINI_API_KEY="your-gemini-api-key"

# GitHub
export GITHUB_PAT="your-github-personal-access-token"

# Google Search
export SEARCH_ENGINE_ID="your-search-engine-id"
export SEARCH_API_KEY="your-search-api-key"

# Slack Integration
export SLACK_WEBHOOK_URL="your-slack-webhook-url"
export SLACK_APP_TOKEN="your-slack-app-token"
export SLACK_BOT_TOKEN="your-slack-bot-token"
export SLACK_SIGNING_SECRET="your-slack-signing-secret"

# Vertex AI Agent Configuration
export VERTEX_AI_APP_ID="your-vertex-ai-app-id"

# Service Account (if using a service account)
export SERVICE_ACCOUNT="your-service-account-email"

# OAuth (if using OAuth)
export OAUTH_CLIENT_ID="your-oauth-client-id"
export OAUTH_CLIENT_SECRET="your-oauth-client-secret"

# Additional Configuration
export MAX_TOKENS=8192
export TEMPERATURE=0.2
export TOP_P=0.95
export TOP_K=40 